extern fn printf(fmt: cstr, ...): i32;
extern fn exit(code: i32): void;
extern fn SDL_Init(flags: u32): i32;

const SDL_INIT_EVERYTHING := 65535 as u32;

fn debug_log(str: cstr): void {
    printf("%s\n", str);
}

fn main(): i32 {
    if SDL_Init(SDL_INIT_EVERYTHING) != 0 {
        debug_log("failed to init sdl");
        exit(1);
    }
    return 0;
}

