extern fn puts(str: cstr): i32;
extern fn printf(fmt: cstr, ...): i32;

fn multiply(n: i64): i64 {
    return n * 2 as i64;
}

fn get_name(str: cstr): cstr {
    return str;
}

fn create_person(): Person {
    return {
        name = get_name("sussy baka"),
        age = multiply(5 as i64) as i32 + 59,
        mad = {
            a = 0,
            aaa = 0 == 1,
        },
    };
}

fn print_person(age: i32, name: cstr): void {
    printf("%d %s", age, name);
}

struct Mad {
    a: i32;
    aaa: bool;
}

struct Person {
    age: i32;
    name: cstr;
    mad: Mad;
}

fn main(): i32 {
    a := create_person();
    a = {};
    print_person(a.mad.a, a.name);
    return 0;
}
