extern fn printf(fmt: cstr, ...): i32;

fn print_person(p: Person): void {
    printf("p = %d, %s\n", p.age.i, p.name);
}

struct Hello {
    i: *i32;
}

struct Person {
    age: *Hello;
    name: cstr;
}

fn main(): i32 {
    i := 20;

    age := {
        i = i,
    } as Hello;

    p := {
        age = ^age,
        name = "sussy",
    } as Person;

    age_as_num := p.age.i;

    print_person(p);

    return 0;
}
