extern fn puts(str: cstr): i32;
extern fn printf(fmt: cstr, ...): i32;

fn multiply(n: i64): i64 {
    return n * 2 as i64;
}

fn get_name(str: cstr): cstr {
    return str;
}

fn create_person(): Person {
    a := {
        name = "sussy",
        age = 20,
    } as Person;
    return a;
}

//typechecking X va_arg
fn print_number_and_string(d: i32, s: cstr): void {
    printf("%d %s\n", d, s);
}

fn print_person(p: Person): void {
    print_number_and_string(p.age, p.name);
}

struct Person {
    age: i32;
    name: cstr;
}

fn main(): i32 {
    print_person({
        age = 55,
        name = "sussy baka",
    });
    print_person(create_person());
    return 0;
}
