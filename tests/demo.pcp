//typechecking X va_arg
fn print_number_and_string(d: i32 = 20, s: cstr): void {
    printf("%d %s\n", d, s);
}

fn print_person(p: Person): void {
    print_number_and_string(p.age, p.name);
}

struct Person {
    age: i32;
    name: cstr;
}

fn main(): i32 {
    person := {
        age = 55,
        name = "sussy baka",
    } as Person;
    print_person(person);
    person.age = 30;
    print_person(person);
    person = {
        name = "a",
        age = 20
    };
    print_person(person);
    person.name = "to";
    print_person(person);
    return 0;
}
